{
   "name": "web-pdf",
   "description": "This is chrome extension to convert set of webpages to pdf",
   "version": "1.0",
   "manifest_version": 3,
   "action": {
       "default_popup": "index.html",
       "default_title": "Open the popup"
   },
   "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["./lib/js/content.js"]
        }
    ]
   
}